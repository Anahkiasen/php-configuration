{
  "name": "anahkiasen/php-reference-dumper",
  "description": "PHP configuration reference dumper/loader for symfony/config",
  "license": "MIT",
  "keywords": [
    "config",
    "php",
    "dumper"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com",
      "homepage": "http://autopergamene.eu/en"
    }
  ],
  "require": {
    "jeremeamia/superclosure": "^2.2",
    "symfony/config": "^2.8|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "symfony/var-dumper": "^2.8|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Symfony\\Component\\Config\\Definition\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Symfony\\Component\\Config\\Definition\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
